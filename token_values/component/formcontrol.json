{
  "appliance": {
    "checkbox": {
      "size": {
        "value": "{formcontrol.size}",
        "type": "sizing"
      },
      "radius": {
        "value": "{forms.radius.small}",
        "type": "borderRadius"
      },
      "stroke": {
        "width": {
          "value": "{forms.stroke.width}",
          "type": "borderWidth"
        }
      },
      "color": {
        "selected": {
          "normal": {
            "value": "{formcontrol.color.selected.normal}",
            "type": "color"
          },
          "pressed": {
            "value": "{formcontrol.color.selected.pressed}",
            "type": "color"
          },
          "disabled": {
            "value": "{formcontrol.color.selected.disabled}",
            "type": "color"
          }
        },
        "unselected": {
          "value": "{formcontrol.color.unselected}",
          "type": "color"
        },
        "border": {
          "unselected": {
            "normal": {
              "value": "{formcontrol.color.border.unselected.normal}",
              "type": "color"
            },
            "pressed": {
              "value": "{formcontrol.color.border.unselected.pressed}",
              "type": "color"
            },
            "disabled": {
              "value": "{formcontrol.color.border.unselected.disabled}",
              "type": "color"
            }
          }
        }
      },
      "mark": {
        "color": {
          "normal": {
            "value": "{formcontrol.color.onSelected.enabled}",
            "type": "color"
          },
          "disabled": {
            "value": "{formcontrol.color.onSelected.disabled}",
            "type": "color"
          }
        }
      },
      "focusedOutline": {
        "radius": {
          "value": "{appliance.checkbox.radius}+1",
          "type": "borderRadius"
        }
      }
    },
    "radioButton": {
      "stroke": {
        "width": {
          "value": "{forms.stroke.width}",
          "type": "borderWidth"
        }
      },
      "size": {
        "value": "{formcontrol.size}",
        "type": "sizing"
      },
      "color": {
        "selected": {
          "normal": {
            "value": "{formcontrol.color.selected.normal}",
            "type": "color"
          },
          "pressed": {
            "value": "{formcontrol.color.selected.pressed}",
            "type": "color"
          },
          "disabled": {
            "value": "{formcontrol.color.selected.disabled}",
            "type": "color"
          }
        },
        "unselected": {
          "value": "{formcontrol.color.unselected}",
          "type": "color"
        },
        "border": {
          "selected": {
            "normal": {
              "value": "{formcontrol.color.border.selected.normal}",
              "type": "color"
            },
            "pressed": {
              "value": "{formcontrol.color.border.selected.pressed}",
              "type": "color"
            },
            "disabled": {
              "value": "{formcontrol.color.border.selected.disabled}",
              "type": "color"
            }
          },
          "unselected": {
            "normal": {
              "value": "{formcontrol.color.border.unselected.normal}",
              "type": "color"
            },
            "pressed": {
              "value": "{formcontrol.color.border.unselected.pressed}",
              "type": "color"
            },
            "disabled": {
              "value": "{formcontrol.color.border.unselected.disabled}",
              "type": "color"
            }
          }
        }
      }
    },
    "formcontrol": {
      "focusedOutline": {
        "color": {
          "value": "{color.border.focused}",
          "type": "color"
        }
      }
    }
  }
}